<% @terminy.each do |t| %>
	<table class="wyniki">
	<caption class="thead"><%= t.data_spotkania %><caption>
	<tbody>
	<tr class="wyniki"><td class="ngoscie">Goście<td class="ngole">Wynik<td class="ngospodarze">Gospodarze
